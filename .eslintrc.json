{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": "off",
        "linebreak-style": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off",
        "import/no-extraneous-dependencies": "off",
        "no-unused-expressions": "off",
        "guard-for-in": "off",
        "no-restricted-syntax": "off"
    }
}